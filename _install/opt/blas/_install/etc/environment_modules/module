
set	version	"3.5.0 ($plat)"
set	lib	"BLAS-123"
set	libroot "BLASROOT"
proc ModulesHelp { } {
        puts stderr "\tThis module provides access to $lib library\n";
	puts stderr "\t Version $version\n"
}

module-whatis "$lib v$version"

setenv 	     $libroot		$root/$plat

prepend-path LD_LIBRARY_PATH    "$root/$plat/lib"
prepend-path LIBRARY_PATH       "$root/$plat/lib"
prepend-path CPATH		"$root/$plat/include"
prepend-path FPATH		"$root/$plat/include"
prepend-path MANPATH		"$root/man"

puts stderr " > (un)load $lib v$version"

#EOF
